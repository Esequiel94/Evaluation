                android:layout_height="wrap_content"
                android:text="county_province"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/Brewery_county_province"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="14sp" />

        </TableRow>

        <TableRow
            android:id="@+id/Fila_postal_code"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/_postal_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="postal_code"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/Brewery_postal_code"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="14sp" />

        </TableRow>

        <TableRow
            android:id="@+id/Fila_country"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/_country"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="country"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/Brewery_country"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="14sp" />

        </TableRow>

        <TableRow
            android:id="@+id/Fila_longitude"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/_longitude"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="longitude"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/Brewery_longitude"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textSize="14sp" />

        </TableRow>

        <TableRow
            android:id="@+id/Fila_latitude"
